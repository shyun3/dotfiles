{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{.UserName}}@{{.HostName}}:{{.PWD}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u2800\uf0e7\u2800",
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "background": "#ffe9aa",
          "type": "root",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u2800{{ .Icon }}\u2800",
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "background": "#ffffff",
          "type": "os",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "\uf115",
            "style": "full"
          },
          "template": "\u2800{{ .Path }}\u2800",
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "background": "#91ddff",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u2800{{ .HEAD }}\u2800",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "background": "#95ffa4",
          "type": "git",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u2800\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}\u2800",
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "background": "#906cff",
          "type": "python",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u2800\ue20f\u2800",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#ff8080",
          "type": "status",
          "style": "powerline"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "style": "round",
            "threshold": 1000
          },
          "template": "\u2800\uf252 {{ .FormattedMs }}\u2800",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b2",
          "background": "#8800dd",
          "type": "executiontime",
          "style": "powerline",
          "invert_powerline": true
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u2800\uf017 \u2002{{ .CurrentDate | date .Format }}\u2800",
          "foreground": "#fff",
          "powerline_symbol": "\ue0b2",
          "background": "#003543",
          "type": "time",
          "style": "powerline",
          "invert_powerline": true
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "‚ùØ\u2800",
          "foreground": "#007ACC",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
