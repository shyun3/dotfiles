[user]
    name = {{@@ _vars["git_name"] @@}}
    email = {{@@ _vars["git_email"] @@}}
[core]
    excludesfile = {{@@ _dotdrop_dotpath @@}}/.gitignore_global
    autocrlf = input
[fetch]
    prune = 1
[alias]
    l = log --graph --oneline --decorate
    root = rev-parse --show-toplevel
