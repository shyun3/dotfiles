--map-python=+.pyi
--langdef=4DGL
--langmap=4DGL:.4dg.4DG.4Dg
--regex-4DGL=/^[[:space:]]*#constant[[:space:]]+([a-zA-Z_][[:alnum:]_]*)/\1/c,constant/
--regex-4DGL=/^[[:space:]]*func[[:space:]]+([a-zA-Z_][[:alnum:]_]*)/\1/f,function/
--regex-4DGL=/^[[:space:]]*#USE[[:space:]]+([a-zA-Z_][[:alnum:]_]*)/\1/u,use/
